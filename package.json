  "bin": {
    "asini": "./bin/asini.js"
  },
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-register": "^6.7.2",
    "eslint": "^3.10.2",
    "eslint-config-asini": "^1.0.0",
    "fs-extra": "^1.0.0",
    "mocha": "^3.1.2",
    "normalize-newline": "^3.0.0"
