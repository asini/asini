{
  "name": "asini",
  "version": "1.3.0",
  "description": "Tool for managing JavaScript projects with multiple packages",
  "main": "lib/index.js",
  "scripts": {
    "bootstrap": "node bin/asini.js bootstrap",
    "build": "babel src -d lib",
    "dev": "babel -w src -d lib",
    "lint": "eslint . --cache",
    "fix": "eslint . --fix",
    "pretest": "npm run lint",
    "test": "mocha -t 5000",
    "test-all": "node bin/asini.js run test",
    "changelog": "asini-changelog",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/asini/asini.git"
  },
  "author": "Sebastian McKenzie <sebmck@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/asini/asini/issues"
  },
  "dependencies": {
    "async": "^2.1.2",
    "chalk": "^1.1.3",
    "cmd-shim": "^2.0.2",
    "command-join": "^1.1.1",
    "cross-spawn": "^5.0.1",
    "glob": "^7.0.6",
    "inquirer": "^1.2.3",
    "isarray": "^2.0.1",
    "lodash.find": "^4.3.0",
    "lodash.unionwith": "^4.2.0",
    "meow": "^3.7.0",
    "minimatch": "^3.0.0",
    "mkdirp": "^0.5.1",
    "normalize-path": "^2.0.1",
    "object-assign": "^4.0.1",
    "object-assign-sorted": "^1.0.0",
    "pad": "^1.0.0",
    "path-exists": "^3.0.0",
    "progress": "^1.1.8",
    "read-cmd-shim": "^1.0.1",
    "rimraf": "^2.5.2",
    "semver": "^5.1.0",
